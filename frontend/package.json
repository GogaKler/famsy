{
  "name": "famsy-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "author": "gogakler.development@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev:web": "vite --host",
    "docker:dev": "docker compose -f docker/development/docker-compose.yml up --build",
    "docker:dev:build": "docker compose -f docker/development/docker-compose.yml build --no-cache",
    "vue-devtools": "../../node_modules/.bin/vue-devtools",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "tailwind-viewer": "tsc tailwind.config.ts --outDir .ts_compiles && mv .ts_compiles/tailwind.config.js .ts_compiles/tailwind.config.cjs && tailwind-config-viewer -p 8068 -c .ts_compiles/tailwind.config.cjs",
    "generate:api-types": "npx openapi-typescript http://localhost:3000/api/docs-json -o src/shared/api/api.d.ts"
  },
  "dependencies": {
    "@formkit/icons": "^1.6.9",
    "@formkit/vue": "^1.6.9",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "axios": "^1.7.7",
    "openapi-fetch": "^0.10.2",
    "pinia": "^2.2.2",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/axios": "^0.14.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/devtools": "^7.7.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.11.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "openapi-typescript": "^7.0.1",
    "postcss": "^8.4.38",
    "sass": "^1.77.5",
    "sass-loader": "^16.0.4",
    "tailwind-config-viewer": "^2.0.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.4.0",
    "vite": "^5.2.11",
    "vitest": "^1.5.0",
    "vue-tsc": "^2.0.11"
  }
}
